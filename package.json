{
	"name": "hobby-cms",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"dev": "next dev",
		"dev:turbo": "next dev --turbo",
		"build": "next build",
		"start": "next start",
		"postinstall": "npm db:prisma-cockroach:generate",
		"lint": "next lint",
		"lint:fix": "next lint --fix",
		"db:prisma-cockroach:generate": "prisma generate --schema=src/scripts/modules/database/vendors/prisma-cockroach/schema.prisma",
		"db:prisma-cockroach:migrate": "dotenv -e .env.local -- npx prisma migrate dev --name migrate --schema=src/scripts/modules/database/vendors/prisma-cockroach/schema.prisma",
		"secret:create": "openssl rand -base64 32"
	},
	"dependencies": {
		"@emotion/react": "^11.10.5",
		"@emotion/styled": "^11.10.5",
		"@mui/icons-material": "^5.11.0",
		"@mui/material": "^5.11.7",
		"@mui/x-date-pickers": "^5.0.18",
		"@next/font": "13.1.6",
		"@prisma/client": "^4.10.1",
		"@types/node": "18.11.19",
		"@types/react": "18.0.27",
		"@types/react-dom": "18.0.10",
		"date-fns": "^2.29.3",
		"eslint": "8.33.0",
		"eslint-config-next": "13.1.6",
		"linq": "^4.0.1",
		"next": "13.1.6",
		"next-auth": "^4.19.2",
		"react": "18.2.0",
		"react-dom": "18.2.0",
		"typescript": "4.9.5"
	},
	"devDependencies": {
		"dotenv-cli": "^7.0.0",
		"prisma": "^4.10.1",
		"sass": "^1.58.0"
	}
}
